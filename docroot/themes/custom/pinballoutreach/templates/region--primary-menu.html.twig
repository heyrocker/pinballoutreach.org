{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
  ]
%}
{% if content %}
  <div{{ attributes.addClass(classes) }}>
    {{ content }}
    {# <div class="donate-link"><a href="https://www.paypal.com/cgi-bin/webscr?cmd=_donations&business=nicole%40pinballoutreach%2eorg&lc=US&item_name=The%20Pinball%20Outreach%20Project&no_note=0&currency_code=USD&bn=PP%2dDonationsBF%3abtn_donate_LG%2egif%3aNonHostedGuest">Donate Now</a></div> #}
  </div>
{% endif %}
